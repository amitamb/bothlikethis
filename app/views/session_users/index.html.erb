<h1>Listing session_users</h1>

<table>
  <tr>
    <th>Given username</th>
  </tr>

<% for session_user in @session_users %>
  <tr>
    <td><%=h session_user.given_username %></td>
    <td><%= link_to 'Show', session_user %></td>
    <td><%= link_to 'Edit', edit_session_user_path(session_user) %></td>
    <td><%= link_to 'Destroy', session_user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New session_user', new_session_user_path %>
