<h1>Listing topic_attributes</h1>

<table>
  <tr>
    <th>Text</th>
  </tr>

<% for topic_attribute in @topic_attributes %>
  <tr>
    <td><%=h topic_attribute.text %></td>
    <td><%= link_to 'Show', topic_attribute %></td>
    <td><%= link_to 'Edit', edit_topic_attribute_path(topic_attribute) %></td>
    <td><%= link_to 'Destroy', topic_attribute, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New topic_attribute', new_topic_attribute_path %>
